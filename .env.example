# MySQL (local). Create DB first: CREATE DATABASE aa2000;
# Then run: mysql -u root -p aa2000 < scripts/schema.sql
MYSQL_HOST=localhost
MYSQL_USER=root
MYSQL_PASSWORD=
MYSQL_DATABASE=aa2000

# API server port (Vite proxy forwards /api to this)
PORT=3001

# Optional: frontend API base URL (leave empty when using Vite proxy in dev)
# VITE_API_URL=http://localhost:3001
